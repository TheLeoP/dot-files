export PATH="/usr/local/go/bin:$PATH"
export PATH="$HOME/go/bin:$PATH"

export PATH="$PATH:/home/luis/.dotnet/tools"
export PATH="$HOME/.local/bin:$PATH"

export LUA_PATH='/usr/share/lua/5.1/?.lua;/usr/local/share/lua/5.1/?.lua;/usr/local/share/lua/5.1/?/init.lua;/usr/share/lua/5.1/?/init.lua;/usr/local/lib/lua/5.1/?.lua;/usr/local/lib/lua/5.1/?/init.lua;/usr/lib/lua/5.1/?.lua;/usr/lib/lua/5.1/?/init.lua;./?.lua;./?/init.lua;/home/luis/.luarocks/share/lua/5.1/?.lua;/home/luis/.luarocks/share/lua/5.1/?/init.lua'
export LUA_CPATH='/usr/local/lib/lua/5.1/?.so;/usr/lib/lua/5.1/?.so;/usr/local/lib/lua/5.1/loadall.so;/usr/lib/lua/5.1/loadall.so;./?.so;/home/luis/.luarocks/lib/lua/5.1/?.so'
export PATH='/home/luis/.luarocks/bin:/home/luis/.local/share/nvim/mason/bin:/home/luis/.config/nvm/versions/node/v20.18.1/bin:/home/luis/.local/bin:/home/luis/go/bin:/usr/local/go/bin:/home/luis/.local/share/nvim/lazy/fzf/bin:/home/luis/.cargo/bin:/usr/local/bin:/usr/bin:/usr/local/sbin:/usr/lib/jvm/default/bin:/usr/bin/site_perl:/usr/bin/vendor_perl:/usr/bin/core_perl:/home/luis/.dotnet/tools'
